{
  "atomKeymap.promptV3Features": true,
  "advancedNewFile.exclude": {
    "node_modules": true,
    "node_modules_electron": true,
    "dist": true
  },
  "advancedNewFile.showInformationMessages": true,
  "advancedNewFile.convenienceOptions": ["last", "current", "root"],
  "bracketPairColorizer.forceUniqueOpeningColor": true,
  "debug.toolBarLocation": "docked",
  "debug.console.fontSize": 16,
  "editor.fontFamily": "FiraCode Nerd Font, FiraCode-Retina",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.fontWeight": "400",
  "editor.formatOnSave": true,
  "editor.minimap.enabled": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 100,
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "*.erb": "html",
    "django-html": "html",
    "jinja-html": "html"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "files.autoSave": "off",
  "files.associations": {
    "**/*.html": "html",
    "**/templates/**/*.html": "django-html",
    "**/templates/**/*.vue": "vue",
    "**/templates/**/*": "django-txt",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements",
    "*.vue": "vue"
  },
  "git.allowForcePush": true,
  "git.alwaysSignOff": false,
  "git.autoStash": true,
  "git.confirmForcePush": true,
  "git.countBadge": "tracked",
  "git.enableCommitSigning": true,
  "git.fetchOnPull": true,
  "git.inputValidation": "always",
  "git.inputValidationSubjectLength": 50,
  "git.showPushSuccessNotification": true,
  "git.suggestSmartCommit": false,
  "githubIssues.createIssueTriggers": ["@TODO"],
  "githubPullRequests.defaultDeletionMethod.selectLocalBranch": true,
  "githubPullRequests.defaultDeletionMethod.selectRemote": true,
  "githubPullRequests.defaultMergeMethod": "rebase",
  "githubPullRequests.fileListLayout": "tree",
  "githubPullRequests.pullRequestTitle": "custom",
  "githubPullRequests.queries": [
    {
      "label": "Created By Me",
      "query": "is:open author:${user}"
    },
    {
      "label": "Assigned To Me",
      "query": "is:open assignee:${user}"
    },
    {
      "label": "Waiting For My Review",
      "query": "is:open review-requested:${user}"
    }
  ],
  "githubPullRequests.telemetry.enabled": true,
  "gitmoji.additionalEmojis": [],
  "gitmoji.outputType": "code",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jupyter.sendSelectionToInteractiveWindow": true,
  "liveshare.audio.startCallOnShare": true,
  "markdown.preview.fontSize": 16,
  "markdownlint.run": "onSave",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "npm.packageManager": "yarn",
  "path-intellisense.autoSlashAfterDirectory": true,
  "path-intellisense.showHiddenFiles": true,
  "peacock.favoriteColors": [
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "Salesforce Blue",
      "value": "#00A1E0"
    },
    {
      "name": "Heroku Purple",
      "value": "#430098"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#00b3e6"
    },
    {
      "name": "Rust Black",
      "value": "#000000"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "prettier.proseWrap": "always",
  "prettier.singleQuote": false,
  "prettier.vueIndentScriptAndStyle": true,
  "python.analysis.diagnosticPublishDelay": 1000,
  "python.analysis.disabled": [],
  "python.analysis.errors": [
    "inherit-non-class",
    "no-cls-argument",
    "no-self-argument",
    "parameter-already-specified",
    "parameter-missing",
    "positional-argument-after-keyword",
    "positional-only-named",
    "return-in-init",
    "typing-generic-arguments",
    "typing-typevar-arguments",
    "typing-newtype-arguments",
    "unresolved-import",
    "undefined-variable"
  ],
  "python.analysis.warnings": [
    "unknown-parameter-name",
    "variable-not-defined-globally",
    "variable-not-defined-nonlocal"
  ],
  "python.analysis.information": [
    "too-many-function-arguments",
    "too-many-positional-arguments-before-star",
    "no-method-argument"
  ],
  "python.autoComplete.addBrackets": false,
  "python.condaPath": "conda",
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.banditEnabled": false,
  "python.linting.flake8Enabled": false,
  "python.linting.flake8Args": ["--max-line-length", "88"],
  "python.linting.mypyEnabled": false,
  "python.showStartPage": false,
  "python.sortImports.path": "isort",
  "ruby.useBundler": true,
  "ruby.useLanguageServer": true,
  "ruby.lint": {
    "rubocop": {
      "useBundler": true
    }
  },
  "ruby.format": "rubocop",
  "screencastMode.onlyKeyboardShortcuts": true,
  "screencastMode.verticalOffset": 10,
  "scss.lint.duplicateProperties": "error",
  "scss.lint.float": "warning",
  "scss.lint.idSelector": "warning",
  "scss.lint.ieHack": "warning",
  "scss.lint.important": "error",
  "scss.lint.zeroUnits": "warning",
  "shellcheck.enable": true,
  "shellcheck.ignoreFileSchemes": ["git", "gitfs"],
  "shellcheck.run": "onSave",
  "svg.preview.mode": "svg",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.shellArgs.osx": ["-l"],
  "terminal.integrated.splitCwd": "workspaceRoot",
  "todohighlight.exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/bower_components/**",
    "**/build/**",
    "**/.vscode/**",
    "**/_output/**",
    "**/*.min.*",
    "**/*.map"
  ],
  "todohighlight.isCaseSensitive": true,
  "todohighlight.isEnable": true,
  "todohighlight.keywords": [
    {
      "text": "@TODO:",
      "borderRadius": "5px"
    }
  ],
  "vetur.grammar.customBlocks": {
    "docs": "md",
    "i18n": "json",
    "static-query": "graphql",
    "page-query": "graphql"
  },
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "vetur.validation.template": true,
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "activityBar.background": "#000C1D",
    "activityBar.border": "#102a44",
    "sideBar.background": "#001122",
    "sideBar.border": "#102a44",
    "sideBar.foreground": "#8BADC1"
  },
  "workbench.colorTheme": "Night Owl",
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": true,
  "yaml.format.enable": true,
  "yaml.format.singleQuote": true,
  "zenMode.centerLayout": true,
  "zenMode.fullScreen": true,
  "zenMode.hideActivityBar": true,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "zenMode.hideTabs": true,
  "zenMode.restore": false,
  "zenMode.silentNotifications": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "editor.wordWrap": "off"
  },
  "[html]": {
    "editor.wordWrap": "off",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.wordWrap": "off",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      // For ESLint
      "source.fixAll.eslint": true
    }
  },
  "[javascriptreact]": {
    "editor.wordWrap": "off",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[ruby]": {
    "editor.tabSize": 2,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.insertSpaces": true
  },
  "[shellscript]": {
    "editor.wordWrap": "off"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.wordWrap": "off"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  }
}
